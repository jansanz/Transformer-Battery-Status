<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="20dip"
    android:layout_marginBottom="20dip"
    android:stretchColumns="0,1">
    <TableRow>
        <TextView
            android:id="@+id/tablet_battery_title"
            style="@style/TableCellTextView"
            android:text="@string/tablet_battery_title" />
        <TextView
            android:id="@+id/dock_battery_title"
            style="@style/TableCellTextView"
            android:text="@string/dock_battery_title" />
    </TableRow>
    <TableRow android:layout_marginTop="10dip">
        <TextView
            android:id="@+id/tablet_battery_level"
            style="@style/TableCellBatteryLevel"/>
        <TextView
            android:id="@+id/dock_battery_level"
            style="@style/TableCellBatteryLevel" />
    </TableRow>
</TableLayout>